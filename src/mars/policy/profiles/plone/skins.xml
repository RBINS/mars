<?xml version="1.0"?>
<object name="portal_skins" meta_type="Plone Skins Tool" allow_any="False"
   cookie_persistence="False" default_skin="mars.skin"
   request_varname="plone_skin">
 <object name="mars_skin_custom_images" meta_type="Filesystem Directory View"
    directory="mars.skin:skins/mars_skin_custom_images"/>
 <object name="mars_skin_custom_templates"
    meta_type="Filesystem Directory View"
    directory="mars.skin:skins/mars_skin_custom_templates"/>
 <object name="mars_skin_styles" meta_type="Filesystem Directory View"
    directory="mars.skin:skins/mars_skin_styles"/>
 <object name="marsapp_categories" meta_type="Filesystem Directory View"
    directory="marsapp.categories:skins/marsapp_categories"/>
 <object name="marsapp_categories_icons" meta_type="Filesystem Directory View"
    directory="marsapp.policy:skins/marsapp_categories_icons"/>

 <skin-path name="mars.skin"  based-on="Sunburst Theme">
    <layer name="cmf_legacy"
     insert-after="*"/>
    <layer name="plone_3rdParty"
     insert-after="plone_deprecated"
     insert-before="cmf_legacy"/>
    <layer name="plone_deprecated"
     insert-after="plone_login"
     insert-before="plone_3rdParty"/>
    <layer name="plone_login"
     insert-after="plone_content"
     insert-before="plone_deprecated"/>
    <layer name="plone_content"
     insert-after="plone_images"
     insert-before="plone_login"/>
    <layer name="plone_images"
     insert-after="plone_forms"
     insert-before="plone_content"/>
    <layer name="plone_forms"
     insert-after="plone_scripts"
     insert-before="plone_images"/>
    <layer name="plone_scripts"
     insert-after="plone_form_scripts"
     insert-before="plone_forms"/>
    <layer name="plone_form_scripts"
     insert-after="plone_styles"
     insert-before="plone_scripts"/>
    <layer name="plone_styles"
     insert-after="plone_templates"
     insert-before="plone_form_scripts"/>
    <layer name="plone_templates"
     insert-after="plone_prefs"
     insert-before="plone_styles"/>
    <layer name="plone_prefs"
     insert-after="plone_wysiwyg"
     insert-before="plone_templates"/>
    <layer name="plone_wysiwyg"
     insert-after="plone_ecmascript"
     insert-before="plone_prefs"/>
    <layer name="plone_ecmascript"
     insert-after="PasswordReset"
     insert-before="plone_wysiwyg"/>
    <layer name="PasswordReset"
     insert-after="ATContentTypes"
     insert-before="plone_ecmascript"/>
    <layer name="ATContentTypes"
     insert-after="plone_kss"
     insert-before="PasswordReset"/>
    <layer name="plone_kss"
     insert-after="mimetypes_icons"
     insert-before="ATContentTypes"/>
    <layer name="mimetypes_icons"
     insert-after="archetypes_kss"
     insert-before="plone_kss"/>
    <layer name="archetypes_kss"
     insert-after="archetypes"
     insert-before="mimetypes_icons"/>
    <layer name="archetypes"
     insert-after="at_extensions"
     insert-before="archetypes_kss"/>
    <layer name="at_extensions"
     insert-after="CMFEditions"
     insert-before="archetypes"/>
    <layer name="CMFEditions"
     insert-after="cmfeditions_views"
     insert-before="at_extensions"/>
    <layer name="cmfeditions_views"
     insert-after="LanguageTool"
     insert-before="CMFEditions"/>
    <layer name="LanguageTool"
     insert-after="referencebrowser"
     insert-before="cmfeditions_views"/>
    <layer name="referencebrowser"
     insert-after="sunburst_styles"
     insert-before="LanguageTool"/>
    <layer name="sunburst_styles"
     insert-after="sunburst_templates"
     insert-before="referencebrowser"/>
    <layer name="sunburst_templates"
     insert-after="sunburst_images"
     insert-before="sunburst_styles"/>
    <layer name="sunburst_images"
     insert-after="csvreplicata_templates"
     insert-before="sunburst_templates"/>
    <layer name="csvreplicata_templates"
     insert-after="csvreplicata_images"
     insert-before="sunburst_images"/>
    <layer name="csvreplicata_images"
     insert-after="bibliography_list"
     insert-before="csvreplicata_templates"/>
    <layer name="bibliography_list"
     insert-after="bibliography_styles"
     insert-before="csvreplicata_images"/>
    <layer name="bibliography_styles"
     insert-after="bibliography_selenium_tests"
     insert-before="bibliography_list"/>
    <layer name="bibliography_selenium_tests"
     insert-after="bibliography_deprecated"
     insert-before="bibliography_styles"/>
    <layer name="bibliography_deprecated"
     insert-after="tinymce"
     insert-before="bibliography_selenium_tests"/>
    <layer name="tinymce"
     insert-after="cmfplomino_templates"
     insert-before="bibliography_deprecated"/>
    <layer name="cmfplomino_templates"
     insert-after="cmfplomino_images"
     insert-before="tinymce"/>
    <layer name="cmfplomino_images"
     insert-after="cmfplomino_styles"
     insert-before="cmfplomino_templates"/>
    <layer name="cmfplomino_styles"
     insert-after="plomino_tinymce"
     insert-before="cmfplomino_images"/>
    <layer name="plomino_tinymce"
     insert-after="Maps"
     insert-before="cmfplomino_styles"/>
    <layer name="Maps"
     insert-after="plomino_tinymce"
     insert-before="plomino_tinymce"/>
    <layer name="eea_geotags"
     insert-after="bibliography"
     insert-before="kupu_tests"/>
    <layer name="bibliography"
     insert-after="ZipFileTransport"
     insert-before="eea_geotags"/>
    <layer name="ZipFileTransport"
     insert-after="collective_bibliocustomviews_custom"
     insert-before="bibliography"/>
    <layer name="collective_bibliocustomviews_custom"
     insert-after="csvreplicata_styles"
     insert-before="ZipFileTransport"/>
    <layer name="csvreplicata_styles"
     insert-after="querywidget"
     insert-before="collective_bibliocustomviews_custom"/>
    <layer name="querywidget"
     insert-after="marsapp_categories_icons"
     insert-before="csvreplicata_styles" />
    <layer name="marsapp_categories_icons"
     insert-after="mars_skin_styles"
     insert-before="collective_bibliocustomviews_custom"/>
    <layer name="mars_skin_styles"
     insert-after="mars_skin_custom_templates"
     insert-before="marsapp_categories_icons"/>
    <layer name="mars_skin_custom_templates"
     insert-after="mars_skin_custom_images"
     insert-before="mars_skin_styles"/>
    <layer name="marsapp_content_icons" remove="true"/>
    <layer name="marsapp_styles" remove="true"/>
    <layer name="marsapp_custom" remove="true"/>
    <layer name="marsapp_categories"
      insert-after="mars_skin_custom_images"
      insert-before="mars_skin_custom_templates"/>
    <layer name="mars_skin_custom_images"
     insert-after="custom"
     insert-before="marsapp_categories"
/>
    <layer name="custom"
     insert-before="*"/>
 </skin-path>
</object>
